@model YearReportEnterprise
<div id="content-header">
    <h1>企业年度报表统计</h1>
</div>
<div id="breadcrumb">
    <a asp-controller="Home" asp-action="Manage" title="Go to BackHome" class="tip-bottom"><i class="glyphicon glyphicon-home"></i> Home</a>
    <a href="#" class="current">企业年度报表详情</a>
</div>
<div class="container-fluid" id="AllPageLayout">
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <table class="table table-hover table-bordered" style="margin-top:-15px;">
                <thead>
                    <tr>
                        <th>序号</th><td>@Model.Id</td>
                        
                        <th>填写时间</th><td>@Model.WriteTime.Date</td>
                    </tr>
                    <tr>
                        <th>联络员</th><td>@Model.User.UserName</td>
                        
                        <th>报表年度</th><td>@Model.User.CompanyName</td>
                    </tr>
                    <tr>                      
                        <th>企业名称</th><td>@Model.User.RegisteNumber</td>
                        
                        <th>注册号</th><td>@Model.DateTime</td>
                    </tr>
                    <tr>
                        <th>企业联系电话</th><td>@Model.Tel</td>
                        
                        <th>通信地址</th><td>@Model.Address</td>
                    </tr>
                    <tr>
                        <th>邮政编码</th><td>@Model.ZipCode</td>
                        
                        <th>电子邮箱</th><td>@Model.Email</td>
                        
                    </tr>
                    <tr>
                        <th>经营状态</th><td>@Model.OperatState</td>
                        
                        <th>是否有投资信息或购买其他公司股权</th><td>@Model.Investment</td>
                        
                    </tr>
                    <tr>
                        <th>是否有网店</th><td>@Model.Website</td>
                        
                        <th>从业人数</th><td>@Model.EmployeeNum</td>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>
