@model BaseInfo
<div id="content-header">
    <h1>注册号管理</h1>
</div>
<div id="breadcrumb">
    <a asp-controller="Home" asp-action="Manage" title="Go to BackHome" class="tip-bottom"><i class="glyphicon glyphicon-home"></i> Home</a>
    <a asp-controller="Admin" asp-action="DetailsBaseInfo">企业信息</a>
    <a href="#" class="current">当前企业详情</a>
</div>
<div class="container-fluid" id="AllPageLayout">
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <table class="table table-hover table-bordered" style="margin-top:-15px;">
                <thead>
                    <tr>
                        <th>序号</th>
                        <td colspan="5">@Model.Id</td>
                    </tr>
                    <tr>
                        <th>注册号</th>
                        <td colspan="2">@Model.RegisteNumber</td>
                        <th>企业名称</th>
                        <td colspan="2">@Model.CompanyName</td>
                    </tr>
                    <tr>
                        <th>住所</th>
                        <td colspan="5">@Model.Address</td>
                    </tr>
                    <tr>
                        <th>营业期限自</th>
                        <td colspan="2">@Model.BeginTime</td>                        
                        <th>营业期限至</th>
                        <td colspan="2">@Model.EndTime</td>
                    </tr>
                    <tr>
                        <th>经营范围</th>
                        <td colspan="5">@Model.Scope</td>
                    </tr>
                    <tr>
                        <th>登记机关</th>
                        <td colspan="2">@Model.PublicUnit</td>
                        <th>核准日期</th>
                        <td colspan="2">@Model.ApproveTime</td>
                       
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>